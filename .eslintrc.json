{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "@rocketseat/eslint-config/react"
    ],
    "parser": "eslint",
    "overrides": [
        {
            "extends": [
                "@rocketseat/eslint-config/react"
            ],
            "files": [
                "*.ts",
                "*.tsx"
            ]
        }
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
    }
}
